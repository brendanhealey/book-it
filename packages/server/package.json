{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "type": "module",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "compile": "tsc",
    "start": "yarn compile && node ./dist/local.js",
    "sl": "sls invoke local -f graphql",
    "so": "sls offline",
    "sd": "echo 'This is really slow - 10m+' && sls deploy --verbose --aws-s3-accelerate",
    "sdf": "echo 'This is really slow - 10m+' && sls deploy --verbose function -f graphql"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@as-integrations/aws-lambda": "^1.1.0",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
